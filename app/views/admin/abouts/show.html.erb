<div class="page-header">
  <h1>Show About</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Title:</strong></dt>
  <dd><%= @about.title %></dd>
  <dt><strong>Description:</strong></dt>
  <dd><%= simple_format(@about.description) %></dd>
</dl>

<%= link_to "Back", admin_abouts_path, :class => 'btn btn-default'  %>
<%= link_to "Edit", edit_admin_about_path(@about), :class => 'btn btn-default' %>
<%= link_to "Delete", admin_about_path(@about),
              :method => 'delete',
              :data => { :confirm => 'Are you sure?' },
              :class => 'btn btn-danger' %>
